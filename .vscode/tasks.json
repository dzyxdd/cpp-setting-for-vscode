{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C: gcc.exe build active C file",
			"command": "D:/LLVM/bin/gcc.exe",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "D:/LLVM/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: D:/LLVM/bin/gcc.exe"
		},

		{
			"type": "cppbuild",
			"label": "C++: g++.exe build active CPP file",
			"command": "D:/LLVM/bin/g++.exe",
			"args": [
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "D:/LLVM/bin/"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: D:/LLVM/bin/g++.exe"
		}
	]
}